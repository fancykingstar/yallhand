{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 100
      }
    ],
    "prefer-promise-reject-errors": [
      "off"
    ],
    "react/jsx-filename-extension": [
      "off"
    ],
    "react/prop-types": [
      0,
      {}
    ],
    "space-in-parens": [
      0,
      "always"
    ],
    "camelcase": [
      0
    ],
    "no-return-assign": [
      "off"
    ],
    "no-underscore-dangle": [
      0,
      "always"
    ]
  }
}